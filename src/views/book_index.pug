extends layout

block content
  ul
    table(style='width:100%', id='book-table', border='1')
      tr
        th Title
        th Author
        th Year
      for book, i in []
        tr
          td
            a(href='/books/' + book.isbn)= book.title
          td= book.author
          td= book.year
  script(src='/scripts/book/index.js' type='module')
